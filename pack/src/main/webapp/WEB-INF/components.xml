<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
    <alias alias="org.sakaiproject.search.api.SearchService" name="${search.service.impl}"/>
    <alias alias="org.sakaiproject.search.api.SearchIndexBuilder" name="${search.indexbuilder.impl}"/>
    <alias alias="uk.ac.ox.oucs.search.indexing.TaskHandler"
           name="uk.ac.ox.oucs.search.solr.indexing.SolrSplitterProcesses"/>
    <alias alias="uk.ac.ox.oucs.search.queueing.IndexQueueing" name="uk.ac.ox.oucs.search.queueing.IndexQueueingImpl"/>

    <import resource="solr.xml"/>
    <import resource="queueing.xml"/>
    <import resource="contentProducer.xml"/>
</beans>
